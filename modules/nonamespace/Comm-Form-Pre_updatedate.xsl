	<xsl:template match="levelinfo/updatedate">
		<xsl:variable name="fullDateAtts">
			<tempdate>
				<xsl:call-template name="DATE_attributes">
					<xsl:with-param name="date" select="date"/>
				</xsl:call-template>
			</tempdate>

  <xsl:template match="verbatim">

    <!--  Original Target XPath:  verbatim   -->
    <verbatim>
      
      <xsl:apply-templates select="@* | node()"/>
    </verbatim>

  </xsl:template>

  <xsl:template match="@verbatimclass">

    <!--  Original Target XPath:  @verbatimclass   -->
    <xsl:attribute name="verbatimclass">
      <xsl:value-of select="."/>
    </xsl:attribute>

  </xsl:template>


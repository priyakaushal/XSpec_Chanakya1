    <xsl:template match="case:courtloc">
        <xsl:choose>
            <xsl:when test=".[normalize-space()='']"/>
            <xsl:otherwise>
                <jurisinfo:location>
                    <location:city>
                        <!--SS[2017-01-12]: Added below condition for moving text() of case:courtinfo under target element location:city-->
                        <xsl:if test="preceding-sibling::text()[preceding-sibling::case:courtcode and following-sibling::case:courtloc and parent::case:courtinfo]">
                            <xsl:value-of select="ancestor::case:courtinfo/text()[preceding-sibling::case:courtcode and following-sibling::case:courtloc]"/>
                        </xsl:if>
                        <xsl:apply-templates select="@* | node()"/>
                        <!--Chaitanya Sharma :-I have commit repeated apply templte which mai cause of duplicate content reported in webstar 7158671-->
                        
                        <!--                        
                            <xsl:apply-templates select="@* | node()"/>-->
                    </location:city>
                </jurisinfo:location>   
            </xsl:otherwise>
        </xsl:choose>
    </xsl:template>
</xsl:stylesheet>
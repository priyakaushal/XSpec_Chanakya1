  &lt;legisinfo:legisinfo&gt;
   &lt;legisinfo:names&gt;
    &lt;popname&gt;Statutes of the Exchequer&lt;/popname&gt;
    &lt;!-- ETC. --&gt;
   &lt;/legisinfo:names&gt;
  &lt;/legisinfo:legisinfo&gt;
 &lt;/primlawinfo:primlawinfo&gt;
&lt;/legis:head&gt;

            &lt;location:address.line&gt;Mount Road,&lt;/location:address.line&gt;
            &lt;location:city&gt;Madras&lt;/location:city&gt;
        &lt;/location:address&gt;.&lt;/text&gt;
&lt;/p&gt;  

                </pre>
    

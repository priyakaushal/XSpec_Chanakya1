	<!--<xsl:message>UK14_Precedents_toc-ChOf-form.xsl requires manual development!</xsl:message> -->

	<xsl:template match="toc">

		<!--  Original Target XPath:  form:prelim/toc   -->
		<form:prelim>
			<toc>
				<xsl:apply-templates select="@* | node()"/>
			</toc>
		</form:prelim>

	</xsl:template>


</xsl:stylesheet>
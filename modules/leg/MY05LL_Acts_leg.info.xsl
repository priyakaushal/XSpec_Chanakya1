	<xsl:template match="leg:info">
		<!--  Original Target XPath:  primlaw:level/primlaw:levelinfo/primlawinfo:primlawinfo/legisinfo:legisinfo   -->
		<primlaw:level leveltype="unclassified">
			<primlaw:levelinfo>

                &lt;ref:locator-params&gt; 
                  &lt;proc:param name="dpsi" value="0QQI"/&gt;
                &lt;/ref:locator-params&gt;
              &lt;/ref:locator&gt;
            &lt;/ref:lnlink&gt;
          &lt;/textitem&gt;
        &lt;/bodytext&gt;
      &lt;/cttr:refitem&gt;
    &lt;/cttr:refsection&gt;

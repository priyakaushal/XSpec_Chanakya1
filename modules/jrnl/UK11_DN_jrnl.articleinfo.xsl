	<xsl:template match="jrnl:articleinfo">

		<!--  Original Target XPath:  doc:metadata/pubinfo:pubinfo   -->
		
				<xsl:apply-templates select="@* | node()"/>

	</xsl:template>

	<xsl:template match="pubnote">

		<!--  Original Target XPath:  disclaimer/bodytext/p/text   -->
		<disclaimer>
			<bodytext>
				<p>
					<text>
					  <xsl:apply-templates select="@* | node() except copyright"/>
					</text>
				</p>
			</bodytext>
		</disclaimer>

	</xsl:template>

	
	


    <xsl:template match="case:citations">
            <cttr:citations>
                <xsl:apply-templates select="@* | node()"/>
            </cttr:citations>
    </xsl:template>
    
    <xsl:template match="leg:citations">
        <cttr:legisinfo>
            <cttr:citations>
                <xsl:apply-templates select="@* | node()"/>
            </cttr:citations>
            <xsl:apply-templates select="parent::source_cttr:annot/source_cttr:use-group"/>
        </cttr:legisinfo>
    </xsl:template>
    
    <xsl:template match="case:citetext">
        <xsl:apply-templates select="@* | node()"/>        
    </xsl:template>
    
    <xsl:template match="leg:citetext">
        <xsl:apply-templates select="@* | node()"/>
    </xsl:template>

</xsl:stylesheet>
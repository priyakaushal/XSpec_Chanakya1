    <!--Vikas Rohilla : Tempalte to match the case:subhist and creates the casehist:casehist/casehist:subhist/casehist:summary -->
    <xsl:template match="case:subhist">
        <casehist:casehist>
            <casehist:subhist>
                <casehist:summary>
                    <xsl:apply-templates select="@* | node()"/>
                </casehist:summary>
            </casehist:subhist>
        </casehist:casehist>
    </xsl:template>

</xsl:stylesheet>

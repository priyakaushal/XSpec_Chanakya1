	<!-- Vikas Rohilla : template match for the case:embeddedcase-->
	<xsl:template match="case:embeddedcase">
		<seclaw:digestgrp>
			<seclaw:digest>
		<courtcase:caseexcerpt>
			<xsl:apply-templates select="@*|node()"/>
		</courtcase:caseexcerpt>
			</seclaw:digest>
			
		</seclaw:digestgrp>
	</xsl:template>
	
	<xsl:template match="case:headnote">
		<courtcase:head>
			<xsl:apply-templates select="@*|node() except catchwordgrp"/>
		</courtcase:head>
	</xsl:template>

<xsl:template match="prelim">
	<courtcase:prelim>
		<bodytext>
			<xsl:apply-templates select="node()"/>
		</bodytext>
	</courtcase:prelim>
	
</xsl:template>

	<xsl:template match="case:disposition">
		<casesum:summaries>
		<casesum:disposition>
			<p>
				<text>
					<xsl:apply-templates/>		
				</text>
			</p>			
		</casesum:disposition>
		</casesum:summaries>
	</xsl:template>
	
	<xsl:template match="case:disposition[parent::case:judgments]">
		<casesum:disposition>
			<p>
				<text>
					<xsl:apply-templates/>		
				</text>
			</p>			
		</casesum:disposition>
	</xsl:template>


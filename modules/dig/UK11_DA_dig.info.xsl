<!--Vikas Rohilla : Template to match the dig:info   -->
	<xsl:template match="dig:info">
	  <xsl:apply-templates select="@* | node() except (dig:subject[@display-name='Jurisdiction']|dig:cite|dig:type|pubdate[@display-name='Published Date'])"/>
	</xsl:template>
  
  
  <xsl:template match="dig:body/dig:info[1]">
    <xsl:apply-templates select="dig:title"/>
    <xsl:choose>
      <xsl:when test="child::leg:info">
        <xsl:apply-templates select="@* | node() except (dig:subject[@display-name='Jurisdiction']|contrib[@contribtype='author']|dig:title|dig:cite|dig:type|pubdate[@display-name='Published Date'])"/>
      </xsl:when>
      <xsl:when test="child::pubdate[@display-name='Issue Date'] or dig:subject[@display-name='Website'] or publication or contrib[@contribtype='issuing-department'] or jrnl:articleinfo/jrnl:journalcite">
        <abstract:target>
          <pubinfo:pubinfo>
            <xsl:apply-templates select="@* | node() except (dig:subject[@display-name='Jurisdiction']|contrib[@contribtype='author']|dig:title|dig:cite|dig:type|pubdate[@display-name='Published Date']|references)"/>
          </pubinfo:pubinfo>
          <xsl:apply-templates select="contrib[@contribtype='author']"/>
        </abstract:target>
      </xsl:when>
      <xsl:otherwise>
      </xsl:otherwise>
    </xsl:choose>
    <xsl:apply-templates select="references"/>
  </xsl:template>
  
  

</xsl:stylesheet>
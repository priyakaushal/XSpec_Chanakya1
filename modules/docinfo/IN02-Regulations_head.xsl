              &lt;lnci:jurisinfo&gt;
                &lt;lnci:in/&gt;
              &lt;/lnci:jurisinfo&gt;
              &lt;lnci:hier&gt;
                &lt;lnci:hierlev label="section" num="8"/&gt;
              &lt;/lnci:hier&gt;
            &lt;/lnci:sesslawinfo&gt;
            &lt;lnci:sesslawref&gt;
              &lt;lnci:standardname normpubcode="IN1926TRUA02"/&gt;
            &lt;/lnci:sesslawref&gt;
          &lt;/lnci:sesslaw&gt;
          &lt;lnci:content&gt;8. Fees.&lt;/lnci:content&gt;
        &lt;/lnci:cite&gt;
      &lt;/ref:cite4thisresource&gt;
    &lt;/ref:citations&gt;
&lt;/regulation:head&gt;

      </pre>
    </section>    
